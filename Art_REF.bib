doi = {10.1177/1475921718757405},
issn = {14759217 (ISSN)},
journal = {Structural Health Monitoring},
keywords = {Civil infrastructures,Computer vision,Data anomalies,Data handling,Data mining,Data visualization,Deep learning,Deep neural networks,Hot research topics,Monitoring,Stacked autoencoder,Structural analysis,Structural health monitoring,Structural health monitoring systems,Structural heath monitoring,Time series signals,Training procedures,Visualization,computer vision,data anomaly detection,deep learning,stacked autoencoder deep neural network},
number = {2},
pages = {401--421},
publisher = {SAGE Publications Ltd},
