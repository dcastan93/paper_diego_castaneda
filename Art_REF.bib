@Article{Hu2011,
  author    = {Hu, Chaoquan and Qiao, Liang and Tian, Hongwei and Lu, Xianyi and Jiang, Qing and Zheng, Weitao},
  title     = {{Role of carbon in the formation of hard Ge1-xCx thin films by reactive magnetron sputtering}},
  journal   = {Physica B: Condensed Matter},
  year      = {2011},
  volume    = {406},
  number    = {13},
  pages     = {2658--2662},
  issn      = {09214526},
  abstract  = {We have deposited germanium carbide (Ge1-xCx) films on Si(1 0 0) substrate via radio-frequency (RF) reactive magnetron sputtering in a CH4/Ar mixture discharge, and explored the effects of carbon content (x) on the chemical bonding and hardness for the obtained films. We find that x significantly influences the chemical bonding, which leads to a pronounced change in the hardness of the film. To reveal the relationship between the chemical bonding and hardness, first-principles calculations have been carried out. It is shown that as x increases from 0 to 0.33, the fraction of sp3 CGe bonds in the film increases at the expense of GeGe bonds, which promotes formation of a strong covalently bonded network, and thus enhances the hardness of the film. However, as x further increases from 0.33 to 0.59, the fraction of sp3 CGe bonds in the film gradually reduces, while that of sp3 CH and graphite-like sp2 CC bonds increases, which damages the compact network structure, resulting in a sharp decrease in the hardness. This investigation suggests that the medium x (0.17{\textless}x{\textless}0.40) is most favorable to the preparation of hard Ge 1-xCx films due to the formation of dominant sp 3 CGe bonds. {\textcopyright} 2011 Elsevier B.V. All rights reserved.},
  doi       = {10.1016/j.physb.2011.01.077},
  file      = {:C$\backslash$:/Users/dcast/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Hu et al. - 2011 - Role of carbon in the formation of hard Ge1-xCx thin films by reactive magnetron sputtering.pdf:pdf},
  keywords  = {Ge1-xCx films,Hardness,Reactive magnetron sputtering},
  publisher = {Elsevier},
  url       = {http://dx.doi.org/10.1016/j.physb.2011.01.077},
}

@Article{Han2015b,
